  <connectionStrings>
    <add name="DBConnection" connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=PIS_CONTACT;Integrated Security=True;" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <system.serviceModel>
     <serviceHostingEnvironment multipleSiteBindingsEnabled="true" /> 
    <behaviors>
      <serviceBehaviors>
        <behavior name="">
          <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="false" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <services>
      <service name="lab07b_wcf_console.ContactsService">
        <endpoint address="" binding="basicHttpBinding" contract="lab07b_wcf_console.IContactsService">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8733/Design_Time_Addresses/lab07b_wcf_console/ContactsService/" />
          </baseAddresses>
        </host>
      </service>
    </services>
  </system.serviceModel>
